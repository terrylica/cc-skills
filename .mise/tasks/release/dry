#!/usr/bin/env bash
#MISE description="Dry-run release (no changes)"
#MISE depends=["release:preflight"]
set -euo pipefail

echo "═══════════════════════════════════════════════════════════"
echo "  DRY RUN (no changes will be made)"
echo "═══════════════════════════════════════════════════════════"

semantic-release --no-ci --dry-run
